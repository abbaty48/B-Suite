{
  "name": "B-Suite",
  "productName": "B-Suite",
  "version": "1.0.0",
  "description": "B-Suite (stand for Business-Suite) is an application for the most business application, such as, POS (Point Of Sale), IMS (Inventory Management System), CRM (Customer Relationship Management), RMS (Retail Management System), EM (Employee Management), A (Accounting)",
  "main": ".webpack/main/index.js",
  "scripts": {
    "start": "npm-run-all --parallel compile-server start-server",
    "start-main": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "compile-server": "npx ttsc appServer/index.ts --outDir .webpack/",
    "start-server": "npm run compile-server && node .webpack/appServer/index.js",
    "codegen": "graphql-codegen --config appServer/src/commons/commons.codegen.ts"
  },
